#include "Console.au3"
SetTitle("我的第一个控制台程序！") ;设置标题
cout("你好！"&@TAB)
CoutLine("欢迎使用AUTOIT控制台UDF!","亮青色") ;颜色参数可以使用数字
Cout("请输入你的名字:")
Local $name = cin()
Cout("请输入你的年龄:","黑色","亮红色")
Local $age = cin()
CoutLine("欢迎:"&$name&" 你的年龄是:"&$age,"亮黄色")
CoutLine("显示常用的cmd命令:")
System("pause")
Cout("","亮紫色") ;设置下面命令的颜色
System("help")
System("pause")
System("cls") ;清屏
Do
        CoutLine("请输入Y,退出程序！")
Until Getch()="y"