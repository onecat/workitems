Global $addr
$addr=InputBox("环境设置","====请输入上网环境===="&@CRLF&@CRLF&"在公司上网请输入：1"&@CRLF&"在家上网请输入：2")
Select
        Case $addr=1
                _Proxy_add()
        Case $addr=2
                _Proxy_del()
        Case Else
                MsgBox(0,"错误","输入错误！")
EndSelect

Func _Proxy_add()
;设置代理脚本
Local $sAddr="http://www.au3.cc" ;将地址换成你的代理脚本地址
RegWrite("HKCU\Software\Microsoft\Windows\CurrentVersion\Internet Settings","AutoConfigURL","REG_SZ",$sAddr)
Local $sVar = RegRead("HKCU\Software\Microsoft\Windows\CurrentVersion\Internet Settings", "AutoConfigURL")
MsgBox(0, "设置代理", "已设置当前代理脚本为："&$sVar)
EndFunc

Func _Proxy_del()
;删除代理脚本
RegDelete("HKCU\Software\Microsoft\Windows\CurrentVersion\Internet Settings","AutoConfigURL")
MsgBox(0, "设置代理", "代理脚本已删除!")
EndFunc